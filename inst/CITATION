  entry    = "Manual",
  title    = "{dmlalg}: Double machine learning algorithms",
  author   = c(person(given = "Corinne", family = "Emmenegger")),
  year     = "2021",
  url  = "https://CRAN.R-project.org/package=dmlalg",
  note = "R-package available on CRAN",
  textVersion = paste("Emmenegger, C. (2021). dmlalg: Duble machine learning algorithms. URL https://CRAN.R-project.org/package=dmlalg. R-package available on CRAN.")
)